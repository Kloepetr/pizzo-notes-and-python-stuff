        "# Looping Through All Key-Value Pairs\n",
        "user_0 = {\n",
        "    'username': 'efermi',\n",
        "    'first': 'enrico',\n",
        "    'last': 'fermi',\n",
        "    }\n",
        "for key, value in user_0.items():\n",
        "print(f\"\\nKey: {key}\")\n",
        "print(f\"Value: {value}\")"
      ]
